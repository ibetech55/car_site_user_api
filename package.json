{
  "name": "car_sire_user_api",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "start": "NODE_ENV=production node build/server.js",
    "start-dev": "NODE_ENV=development node build/server.js",
    "dev": "NODE_ENV=development ts-node-dev --transpile-only --ignore-watch node_modules --respawn src/server.ts",
    "migration:generate": "ts-node ./node_modules/typeorm/cli.js migration:create",
    "migration-prod:run": "NODE_ENV=production ts-node ./node_modules/typeorm/cli.js migration:run -d ./src/Infra/Database/connection.ts",
    "migration:run": "NODE_ENV=developement ts-node ./node_modules/typeorm/cli.js migration:run -d ./src/Infra/Database/connection.ts",
    "migration:revert": "ts-node ./node_modules/typeorm/cli.js migration:revert -d /src/Infra/Database/connection.ts",
    "build": "NODE_ENV=developement rm -rf build && npx tsc",
    "build-prod": "NODE_ENV=production rm -rf build && npx tsc"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.460.0",
    "@aws-sdk/s3-request-presigner": "^3.460.0",
    "@types/luxon": "^3.4.2",
    "amqplib": "^0.10.3",
    "axios": "^1.5.1",
    "bcryptjs": "^2.4.3",
    "cookie": "^0.6.0",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "crypto-browserify": "^3.12.0",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-async-errors": "^3.1.1",
    "express-fileupload": "^1.4.1",
    "jest": "^29.5.0",
    "jsonwebtoken": "^9.0.2",
    "luxon": "^3.4.4",
    "morgan": "^1.10.0",
    "nock": "^13.3.6",
    "pg": "^8.10.0",
    "reflect-metadata": "^0.1.13",
    "supertest": "^6.3.3",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0",
    "typeorm": "^0.3.16",
    "uuid": "^9.0.0",
    "yamljs": "^0.3.0"
  },
  "devDependencies": {
    "@types/amqplib": "^0.10.4",
    "@types/bcryptjs": "^2.4.5",
    "@types/cookie": "^0.6.0",
    "@types/cookie-parser": "^1.4.3",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/express-fileupload": "^1.4.3",
    "@types/jest": "^29.5.1",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/morgan": "^1.9.4",
    "@types/node": "^20.10.0",
    "@types/supertest": "^2.0.15",
    "@types/uuid": "^9.0.1",
    "@types/yamljs": "^0.2.33",
    "axios-mock-adapter": "^1.22.0",
    "mocha": "^10.2.0",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.5.0",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.2.2",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4"
  }
